using System;

namespace Homework
{
    class ProblemSix
    {
        static void Main(string[] args)
        {
            Console.WriteLine(FindSqrOfSumToN(100) - FindSumOfSqrToN(100));
        }

        static private long FindSumOfSqrToN(int n)
        {
            return n * (n + 1) * (2 * n + 1) / 6;
        }

        static private long FindSqrOfSumToN(int n)
        {
            return n * (n + 1) * n * (n + 1) / 4;
        }
    }
}
